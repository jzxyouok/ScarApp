<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap>
 	<typeAlias alias="wconfValueclass" type="com.zero2ipo.framework.conf.bo.WconfValue" />
	<resultMap id="wconfValue" class="wconfValueclass" >
		<result column="CONF_KEY" property="confKey"/>
		<result column="CONF_VALUE" property="confValue"/>
	</resultMap>
	
	<!-- 获取全局参数Map集合:还需考虑一个内外网的条件过滤 -->
	<select id="findGconfMap" resultMap ="wconfValue">
		SELECT 
			T.CONF_KEY,T.CONF_VALUE 
		FROM 
			S9_SYS_CONF_VALUE T
	</select>
	
</sqlMap>